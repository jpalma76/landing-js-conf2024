---
import Section from "../components/Section.astro";


---
<Section>
    <div class="sponsorRank-container">
        <div class="gold-sponsor">

        </div>

        <div class="bronze-sponsor">

        </div>
    </div>
</Section>

<style>
    .sponsorRank-container {
        width: 1136px;
        height: 600px;
        display: flex;
        flex-direction: column;
        gap: 30px;
    }
    
    .gold-sponsor {
        width: 100%;
        height: 300px;
        border: 1px solid #efe060;
        border-radius: 24px;
        background-image: linear-gradient(.07deg,#50481d33 .03%,#0000 95.96%),radial-gradient(95.25% 166.54% at 98.39% 2.85%,#3635269e 8%,#09090700 80%);
    }

    .bronze-sponsor {
        width: 100%;
        height: 300px;
        border: 1px solid #efe060;
        border-radius: 24px;
        background-image: linear-gradient(#0000 4.04%,#361c1333 99.97%),radial-gradient(95.25% 166.54% at 98.39% 2.85%,#361c1333 8%,#0000 80%);
    }
</style>